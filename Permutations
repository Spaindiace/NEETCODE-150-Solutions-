class Solution {
public:
    vector<vector<int>> permute(vector<int>& nums) {
        int n = nums.size();

        vector<vector<int>> vp;
        int j = 0;
        sort(nums.begin(),nums.end());
        do {
           vector<int>temp;
            for (int i = 0; i < n; i++) {
               temp.push_back(nums[i]);
            }
            vp.push_back(temp);
            temp.clear();

        } while (next_permutation(nums.begin(), nums.end()));
        return vp;
    }
};
