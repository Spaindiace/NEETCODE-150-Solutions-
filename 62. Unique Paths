class Solution {
public:
    int uniquePaths(int m, int n) {
        vector<vector<int>> mat(m, vector<int>(n, 0));
        for (int i = 0; i < n; i++) {
            mat[0][i] = 1;
        }
        for (int j = 1; j < m; j++) {
            mat[j][0] = 1;
        }
        for (int i = 0; i < m; i++) {
            for (int j = 0; j < n; j++) {
                if (mat[i][j] != 1) {
                    mat[i][j] = mat[i - 1][j] + mat[i][j - 1];
                } else {
                    continue;
                }
            }
        }
        return mat[m - 1][n - 1];
    }
};
